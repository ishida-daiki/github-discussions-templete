{
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ishida-daiki/github-discussions-templete"
  },
  "homepage": "https://github.com/ishida-daiki/github-discussions-templete/blob/main/README.md",
  "author": "ishida-daiki",
  "license": "MIT",
  "dependencies": {
    "@create-figma-plugin/ui": "^3.2.0",
    "@create-figma-plugin/utilities": "^3.2.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@create-figma-plugin/build": "^3.2.0",
    "@create-figma-plugin/tsconfig": "^3.2.0",
    "@figma/plugin-typings": "1.90.0",
    "@testing-library/preact-hooks": "^1.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "preact": "^10.23.2",
    "preact-render-to-string": "^6.5.10",
    "typescript": ">=4"
  },
  "figma-plugin": {
    "editorType": [
      "figma",
      "dev",
      "figjam"
    ],
    "id": "Discussions Templete",
    "name": "Discussions Templete",
    "main": "src/main.ts",
    "ui": "src/app/App.tsx",
    "api": "1.0.0",
    "enablePrivatePluginApi": true,
    "capabilities": [
      "inspect",
      "vscode"
    ],
    "enableProposedApi": false,
    "documentAccess": "dynamic-page",
    "permissions": [
      "currentuser"
    ],
    "networkAccess": {
      "allowedDomains": [
        "https://api.github.com/graphql",
        "https://api.github.com"
      ]
    }
  }
}
